{
    "name": "harmonylang",
    "displayName": "HarmonyLang",
    "description": "Syntax support for RvR's Harmony",
    "version": "0.0.1",
    "publisher": "kevinsun-dev-cornell",
    "repository": "https://github.com/kevinsun-dev/harmonylang",
    "engines": {
        "vscode": "^1.48.0"
    },
    "categories": [
        "Programming Languages"
    ],
    "keywords": [
        "CS 4410",
        "Harmony",
        "Syntax",
        "Language"
    ],
    "author": {
        "email": "contact@kevinsun.dev",
        "name": "Kevin Sun"
    },
    "contributes": {
        "languages": [{
            "id": "harmony",
            "aliases": ["Harmony", "harmony"],
            "extensions": [".hny"],
            "configuration": "./language-configuration.json"
        }],
        "grammars": [{
            "language": "harmony",
            "scopeName": "source.harmony",
            "path": "./syntaxes/harmony.tmLanguage.json"
        }]
    }
}